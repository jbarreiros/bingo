{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
      "prettier",
      "eslint:recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "globals": {
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error"
    }
}
